{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "profilePhoto": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "company",
        "employee"
      ]
    },
    "company": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::company.company",
      "inversedBy": "users"
    },
    "ownerCompany": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::company.company",
      "inversedBy": "ownerUser"
    },
    "ownerBoards": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::board.board",
      "mappedBy": "author"
    },
    "comentaries": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comentary.comentary",
      "mappedBy": "author"
    },
    "ownerProjects": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project.project",
      "mappedBy": "ownerUser"
    },
    "projects": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::project.project",
      "inversedBy": "users"
    },
    "responsibleboards": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::board.board",
      "mappedBy": "responsible"
    },
    "knowledges": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::knowledge.knowledge",
      "mappedBy": "author"
    }
  }
}
